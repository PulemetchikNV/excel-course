{
    "parser" : "babel-eslint",
    "env" : {
        "es6" : true,   
        "browser": true,
        "node" : true
    },
    "rules":{
        "semi": "off",
        "arrow-parents": "off",
        "coma-dangle": "off",
        "require-jsdoc" : "off",
        "linebreak-style": 0,
        "eol-last": 0,
        "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }],
    },
    "extends": "google"
}